<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appdsn.pullrefreshdemo"
    android:versionCode="2"
    android:versionName="2.0">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.appdsn.pullrefreshdemo.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.appdsn.pullrefreshdemo.ListViewActivity" />
        <activity android:name="com.appdsn.pullrefreshdemo.RecyclerViewActivity" />
        <activity android:name="com.appdsn.pullrefreshdemo.TextViewActivity" />
        <activity android:name="com.appdsn.pullrefreshdemo.WebActivity" />
        <activity android:name="com.appdsn.pullrefreshdemo.ScrollActivity" />
    </application>

</manifest>